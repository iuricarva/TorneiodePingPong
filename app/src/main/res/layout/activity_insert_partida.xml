<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".InsertPartidaActivity">

    <android.support.design.widget.FloatingActionButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:clickable="true" app:srcCompat="@android:drawable/ic_menu_delete"
            android:id="@+id/btnDeletePartida"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="5dp"
            app:layout_constraintTop_toBottomOf="@+id/btnSavePartida"
            android:layout_marginLeft="270dp" android:layout_marginStart="270dp"/>
    <Spinner
            android:layout_width="253dp"
            android:layout_height="47dp"
            android:id="@+id/spTecnico1" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
            android:layout_marginRight="8dp" app:layout_constraintStart_toEndOf="@+id/textView3"
            android:layout_marginLeft="8dp" android:layout_marginStart="8dp" android:layout_marginTop="28dp"
            app:layout_constraintTop_toBottomOf="@+id/textView12" app:layout_constraintHorizontal_bias="0.38"/>
    <Spinner
            android:layout_width="248dp"
            android:layout_height="44dp"
            android:id="@+id/spTecnico2" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="16dp"
            android:layout_marginRight="16dp" android:layout_marginTop="24dp"
            app:layout_constraintTop_toBottomOf="@+id/btnMenos1"/>
    <TextView
            android:text="Técnico 1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView3" android:textSize="18sp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="16dp" android:layout_marginStart="16dp" android:layout_marginTop="40dp"
            app:layout_constraintTop_toBottomOf="@+id/textView12"/>
    <TextView
            android:text="Pontuação"
            android:layout_width="87dp"
            android:layout_height="wrap_content"
            android:id="@+id/textView10" android:textSize="18sp"
            android:layout_marginTop="20dp" app:layout_constraintTop_toBottomOf="@+id/spTecnico1"
            android:layout_marginStart="40dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="40dp"
            app:layout_constraintEnd_toStartOf="@+id/btnMenos1"
            app:layout_constraintHorizontal_bias="0.0" android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toTopOf="@+id/textView8" app:layout_constraintVertical_bias="0.0"/>
    <TextView
            android:text="Pontuação"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView11" android:textSize="18sp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="48dp" android:layout_marginStart="48dp"
            app:layout_constraintEnd_toStartOf="@+id/btnMenos2"
            android:layout_marginTop="32dp"
            app:layout_constraintTop_toBottomOf="@+id/spTecnico2" app:layout_constraintHorizontal_bias="0.8"
            android:layout_marginBottom="8dp" app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintVertical_bias="0.016"/>
    <TextView
            android:text="Técnico 2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView8" android:textSize="18sp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="8dp" android:layout_marginStart="8dp"
            app:layout_constraintEnd_toStartOf="@+id/spTecnico2" android:layout_marginEnd="8dp"
            android:layout_marginRight="8dp" android:layout_marginTop="32dp"
            app:layout_constraintTop_toBottomOf="@+id/btnMenos1" app:layout_constraintHorizontal_bias="0.307"/>
    <Button
            android:text="-"
            android:layout_width="40dp"
            android:layout_height="49dp"
            android:id="@+id/btnMenos1" android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/spTecnico1"
            app:layout_constraintEnd_toStartOf="@+id/ptTecnico1"
            android:layout_marginLeft="19dp" android:layout_marginStart="19dp"
            app:layout_constraintStart_toEndOf="@+id/textView10"/>
    <Button
            android:text="-"
            android:layout_width="40dp"
            android:layout_height="49dp"
            android:id="@+id/btnMenos2" app:layout_constraintEnd_toStartOf="@+id/ptTecnico1"
            android:layout_marginTop="20dp"
            app:layout_constraintTop_toBottomOf="@+id/spTecnico2"
            android:layout_marginBottom="8dp" app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintVertical_bias="0.017" app:layout_constraintStart_toEndOf="@+id/textView11"
            android:layout_marginLeft="19dp" android:layout_marginStart="19dp"/>
    <Button
            android:text="+"
            android:layout_width="40dp"
            android:layout_height="49dp"
            android:id="@+id/btnMais1" android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/spTecnico1" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="8dp" android:layout_marginRight="8dp"
            app:layout_constraintStart_toEndOf="@+id/ptTecnico1"
            app:layout_constraintHorizontal_bias="0.0"/>
    <Button
            android:text="+"
            android:layout_width="40dp"
            android:layout_height="49dp"
            android:id="@+id/btnMais2" android:layout_marginTop="20dp"
            app:layout_constraintTop_toBottomOf="@+id/spTecnico2" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="12dp" android:layout_marginRight="12dp" android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintVertical_bias="0.017"
            app:layout_constraintStart_toEndOf="@+id/ptTecnico1" android:layout_marginLeft="4dp"
            android:layout_marginStart="4dp" app:layout_constraintHorizontal_bias="0.59"/>
    <EditText
            android:layout_width="75dp"
            android:layout_height="49dp"
            android:inputType="number"
            android:ems="10"
            android:id="@+id/ptTecnico1" android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/spTecnico1" app:layout_constraintStart_toEndOf="@+id/btnMenos1"
            app:layout_constraintEnd_toStartOf="@+id/btnMais1" android:text="0" android:editable="false"/>
    <EditText
            android:layout_width="74dp"
            android:layout_height="49dp"
            android:inputType="number"
            android:ems="10"
            android:id="@+id/ptTecnico2"
            app:layout_constraintEnd_toStartOf="@+id/btnMais2"
            android:layout_marginTop="20dp"
            app:layout_constraintTop_toBottomOf="@+id/spTecnico2"
            android:layout_marginBottom="8dp" app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintVertical_bias="0.017" android:text="0"
            app:layout_constraintStart_toEndOf="@+id/btnMenos2" android:layout_marginLeft="8dp"
            android:layout_marginStart="8dp" app:layout_constraintHorizontal_bias="0.181" android:editable="false"/>
    <TextView
            android:text="Partida"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView12" android:textSize="24sp"
            android:layout_marginTop="30dp" app:layout_constraintTop_toTopOf="parent" android:layout_marginEnd="8dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginRight="8dp" android:layout_marginStart="8dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="8dp"/>
    <android.support.design.widget.FloatingActionButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:clickable="true" app:srcCompat="@android:drawable/ic_menu_save"
            android:id="@+id/btnSavePartida"
            android:layout_marginTop="30dp"
            app:layout_constraintTop_toBottomOf="@+id/btnMais2" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="270dp" android:layout_marginStart="270dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintBottom_toTopOf="@+id/btnDeletePartida"/>
</android.support.constraint.ConstraintLayout>